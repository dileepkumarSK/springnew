package com.two.beans;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class Vehicle {

	
	public void journey() {
		System.out.println("Vehicle.journey()");
		
		ApplicationContext ap=new ClassPathXmlApplicationContext("com/two/cfgs/applicationContext.xml");
		Engine e=ap.getBean(Engine.class);
		e.start();
		e.Stop();
		
	}
	
	public void lights() {
		System.out.println("Vehicle.lights()");
	}
	
	
}
